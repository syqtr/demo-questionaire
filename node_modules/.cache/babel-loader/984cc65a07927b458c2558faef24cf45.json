 return string.replace(/:(\\w+)/g, (match, word) => {\n      if (this[word]) {\n        return '<span class=\"f-string-em\">' + this[word] + '</span>'\n      }\n      \n      return match\n    })\n  }\n}\n\n\n"]}]}